  },
  "slok8": {
    "line1": "कदम्बमञ्जरी-कॢप्त-कर्णपूर-मनोहरा ।",
    "line2": "ताटङ्क-युगली-भूत-तपनोडुप-मण्डला ॥ ८॥"
  },
  "slok9": {
    "line1": "पद्मराग-शिलादर्श-परिभावि-कपोलभूः ।",
    "line2": "नवविद्रुम-बिम्बश्री-न्यक्कारि-रदनच्छदा ॥ ९॥ var. दशनच्छदा"
  },
  "slok10": {
    "line1": "शुद्ध-विद्याङ्कुराकार-द्विजपङ्क्ति-द्वयोज्ज्वला ।",
    "line2": "कर्पूर-वीटिकामोद-समाकर्षि-दिगन्तरा ॥ १०॥"
  },
  "slok11": {
    "line1": "निज-सल्लाप-माधुर्य-विनिर्भर्त्सित-कच्छपी । var. निज-संलाप",
    "line2": "मन्दस्मित-प्रभापूर-मज्जत्कामेश-मानसा ॥ ११॥"
  },
  "slok12": {
    "line1": "अनाकलित-सादृश्य-चिबुकश्री-विराजिता । var. चुबुकश्री",
    "line2": "कामेश-बद्ध-माङ्गल्य-सूत्र-शोभित-कन्धरा ॥ १२॥"
  },
  "slok13": {
    "line1": "कनकाङ्गद-केयूर-कमनीय-भुजान्विता ।",
    "line2": "रत्नग्रैवेय-चिन्ताक-लोल-मुक्ता-फलान्विता ॥ १३॥"
  },
  "slok14": {
    "line1": "कामेश्वर-प्रेमरत्न-मणि-प्रतिपण-स्तनी ।",
    "line2": "नाभ्यालवाल-रोमालि-लता-फल-कुचद्वयी ॥ १४॥"
  },
  "slok15": {
    "line1": "लक्ष्यरोम-लताधारता-समुन्नेय-मध्यमा ।",
    "line2": "स्तनभार-दलन्मध्य-पट्टबन्ध-वलित्रया ॥ १५॥"
  },
  "slok16": {
    "line1": "अरुणारुण-कौसुम्भ-वस्त्र-भास्वत्-कटीतटी ।",
    "line2": "रत्न-किङ्किणिका-रम्य-रशना-दाम-भूषिता ॥ १६॥"
  },
  "slok17": {
    "line1": "कामेश-ज्ञात-सौभाग्य-मार्दवोरु-द्वयान्विता ।",
    "line2": "माणिक्य-मुकुटाकार-जानुद्वय-विराजिता ॥ १७॥"
  },
  "slok18": {
    "line1": "इन्द्रगोप-परिक्षिप्त-स्मरतूणाभ-जङ्घिका ।",
    "line2": "गूढगुल्फा कूर्मपृष्ठ-जयिष्णु-प्रपदान्विता ॥ १८॥"
  },
  "slok19": {
    "line1": "नख-दीधिति-संछन्न-नमज्जन-तमोगुणा ।",
    "line2": "पदद्वय-प्रभाजाल-पराकृत-सरोरुहा ॥ १९॥"
  },
  "slok20": {
    "line1": "सिञ्जान-मणिमञ्जीर-मण्डित-श्री-पदाम्बुजा । var. शिञ्जान",
    "line2": "मराली-मन्दगमना महालावण्य-शेवधिः ॥ २०॥"
  },
  "slok21": {
    "line1": "सर्वारुणाऽनवद्याङ्गी सर्वाभरण-भूषिता ।",
    "line2": "शिव-कामेश्वराङ्कस्था शिवा स्वाधीन-वल्लभा ॥ २१॥"
  },
  "slok22": {
    "line1": "सुमेरु-मध्य-श‍ृङ्गस्था श्रीमन्नगर-नायिका ।",
    "line2": "चिन्तामणि-गृहान्तस्था पञ्च-ब्रह्मासन-स्थिता ॥ २२॥"
  },
  "slok23": {
    "line1": "महापद्माटवी-संस्था कदम्बवन-वासिनी ।",
    "line2": "सुधासागर-मध्यस्था कामाक्षी कामदायिनी ॥ २३॥"
  },
  "slok24": {
    "line1": "देवर्षि-गण-संघात-स्तूयमानात्म-वैभवा ।",
    "line2": "भण्डासुर-वधोद्युक्त-शक्तिसेना-समन्विता ॥ २४॥"
  },
  "slok25": {
    "line1": "सम्पत्करी-समारूढ-सिन्धुर-व्रज-सेविता ।",
    "line2": "अश्वारूढाधिष्ठिताश्व-कोटि-कोटिभिरावृता ॥ २५॥"
  },
  "slok26": {
    "line1": "चक्रराज-रथारूढ-सर्वायुध-परिष्कृता ।",
    "line2": "गेयचक्र-रथारूढ-मन्त्रिणी-परिसेविता ॥ २६॥"
  },
  "slok27": {
    "line1": "किरिचक्र-रथारूढ-दण्डनाथा-पुरस्कृता ।",
    "line2": "ज्वाला-मालिनिकाक्षिप्त-वह्निप्राकार-मध्यगा ॥ २७॥"
  },
  "slok28": {
    "line1": "भण्डसैन्य-वधोद्युक्त-शक्ति-विक्रम-हर्षिता ।",
    "line2": "नित्या-पराक्रमाटोप-निरीक्षण-समुत्सुका ॥ २८॥"
  },
  "slok29": {
    "line1": "भण्डपुत्र-वधोद्युक्त-बाला-विक्रम-नन्दिता ।",
    "line2": "मन्त्रिण्यम्बा-विरचित-विषङ्ग-वध-तोषिता ॥ २९॥"
  },
  "slok30": {
    "line1": "विशुक्र-प्राणहरण-वाराही-वीर्य-नन्दिता ।",
    "line2": "कामेश्वर-मुखालोक-कल्पित-श्रीगणेश्वरा ॥ ३०॥"
  },
  "slok31": {
    "line1": "महागणेश-निर्भिन्न-विघ्नयन्त्र-प्रहर्षिता ।",
    "line2": "भण्डासुरेन्द्र-निर्मुक्त-शस्त्र-प्रत्यस्त्र-वर्षिणी ॥ ३१॥"
  },
  "slok32": {
    "line1": "कराङ्गुलि-नखोत्पन्न-नारायण-दशाकृतिः ।",
    "line2": "महा-पाशुपतास्त्राग्नि-निर्दग्धासुर-सैनिका ॥ ३२॥"
  },
  "slok33": {
    "line1": "कामेश्वरास्त्र-निर्दग्ध-सभण्डासुर-शून्यका ।",
    "line2": "ब्रह्मोपेन्द्र-महेन्द्रादि-देव-संस्तुत-वैभवा ॥ ३३॥"
  },
  "slok34": {
    "line1": "हर-नेत्राग्नि-संदग्ध-काम-सञ्जीवनौषधिः ।",
    "line2": "श्रीमद्वाग्भव-कूटैक-स्वरूप-मुख-पङ्कजा ॥ ३४॥"
  },
  "slok35": {
    "line1": "कण्ठाधः-कटि-पर्यन्त-मध्यकूट-स्वरूपिणी ।",
    "line2": "शक्ति-कूटैकतापन्न-कट्यधोभाग-धारिणी ॥ ३५॥"
  },
  "slok36": {
    "line1": "मूल-मन्त्रात्मिका मूलकूटत्रय-कलेवरा ।",
    "line2": "कुलामृतैक-रसिका कुलसंकेत-पालिनी ॥ ३६॥"
  },
  "slok37": {
    "line1": "कुलाङ्गना कुलान्तस्था कौलिनी कुलयोगिनी ।",
