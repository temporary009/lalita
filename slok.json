{
  "slok1": {
    "line1": "ॐ श्रीमाता श्रीमहाराज्ञी श्रीमत्-सिंहासनेश्वरी ।",
    "line2": "चिदग्नि-कुण्ड-सम्भूता देवकार्य-समुद्यता ॥ १॥"
  },
  "slok2": {
    "line1": "उद्यद्भानु-सहस्राभा चतुर्बाहु-समन्विता ।",
    "line2": "रागस्वरूप-पाशाढ्या क्रोधाकाराङ्कुशोज्ज्वला ॥ २॥"
  },
  "slok3": {
    "line1": "मनोरूपेक्षु-कोदण्डा पञ्चतन्मात्र-सायका ।",
    "line2": "निजारुण-प्रभापूर-मज्जद्ब्रह्माण्ड-मण्डला ॥ ३॥"
  },
  "slok4": {
    "line1": "चम्पकाशोक-पुन्नाग-सौगन्धिक-लसत्कचा ।",
    "line2": "कुरुविन्दमणि-श्रेणी-कनत्कोटीर-मण्डिता ॥ ४॥"
  },
  "slok5": {
    "line1": "अष्टमीचन्द्र-विभ्राज-दलिकस्थल-शोभिता ।",
    "line2": "मुखचन्द्र-कलङ्काभ-मृगनाभि-विशेषका ॥ ५॥"
  },
  "slok6": {
    "line1": "वदनस्मर-माङ्गल्य-गृहतोरण-चिल्लिका ।",
    "line2": "वक्त्रलक्ष्मी-परीवाह-चलन्मीनाभ-लोचना ॥ ६॥"
  },
  "slok7": {
    "line1": "नवचम्पक-पुष्पाभ-नासादण्ड-विराजिता ।",
