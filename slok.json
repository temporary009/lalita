  },
  "slok38": {
    "line1": "मूलाधारैक-निलया ब्रह्मग्रन्थि-विभेदिनी ।",
    "line2": "मणि-पूरान्तरुदिता विष्णुग्रन्थि-विभेदिनी ॥ ३८॥"
  },
  "slok39": {
    "line1": "आज्ञा-चक्रान्तरालस्था रुद्रग्रन्थि-विभेदिनी ।",
    "line2": "सहस्राराम्बुजारूढा सुधा-साराभिवर्षिणी ॥ ३९॥"
  },
  "slok40": {
    "line1": "तडिल्लता-समरुचिः षट्चक्रोपरि-संस्थिता ।",
    "line2": "महासक्तिः कुण्डलिनी बिसतन्तु-तनीयसी ॥ ४०॥"
  },
  "slok41": {
    "line1": "भवानी भावनागम्या भवारण्य-कुठारिका ।",
    "line2": "भद्रप्रिया भद्रमूर्तिर् भक्त-सौभाग्यदायिनी ॥ ४१॥"
  },
  "slok42": {
    "line1": "भक्तिप्रिया भक्तिगम्या भक्तिवश्या भयापहा ।",
    "line2": "शाम्भवी शारदाराध्या शर्वाणी शर्मदायिनी ॥ ४२॥"
  },
  "slok43": {
    "line1": "शाङ्करी श्रीकरी साध्वी शरच्चन्द्र-निभानना ।",
    "line2": "शातोदरी शान्तिमती निराधारा निरञ्जना ॥ ४३॥"
  },
  "slok44": {
    "line1": "निर्लेपा निर्मला नित्या निराकारा निराकुला ।",
    "line2": "निर्गुणा निष्कला शान्ता निष्कामा निरुपप्लवा ॥ ४४॥"
  },
  "slok45": {
    "line1": "नित्यमुक्ता निर्विकारा निष्प्रपञ्चा निराश्रया ।",
    "line2": "नित्यशुद्धा नित्यबुद्धा निरवद्या निरन्तरा ॥ ४५॥"
  },
  "slok46": {
    "line1": "निष्कारणा निष्कलङ्का निरुपाधिर् निरीश्वरा ।",
    "line2": "नीरागा रागमथनी निर्मदा मदनाशिनी ॥ ४६॥"
  },
  "slok47": {
    "line1": "निश्चिन्ता निरहंकारा निर्मोहा मोहनाशिनी ।",
    "line2": "निर्ममा ममताहन्त्री निष्पापा पापनाशिनी ॥ ४७॥"
  },
  "slok48": {
    "line1": "निष्क्रोधा क्रोधशमनी निर्लोभा लोभनाशिनी ।",
    "line2": "निःसंशया संशयघ्नी निर्भवा भवनाशिनी ॥ ४८॥ var. निस्संशया"
  },
  "slok49": {
    "line1": "निर्विकल्पा निराबाधा निर्भेदा भेदनाशिनी ।",
    "line2": "निर्नाशा मृत्युमथनी निष्क्रिया निष्परिग्रहा ॥ ४९॥"
  },
  "slok50": {
    "line1": "निस्तुला नीलचिकुरा निरपाया निरत्यया ।",
    "line2": "दुर्लभा दुर्गमा दुर्गा दुःखहन्त्री सुखप्रदा ॥ ५०॥"
  },
  "slok51": {
    "line1": "दुष्टदूरा दुराचार-शमनी दोषवर्जिता ।",
    "line2": "सर्वज्ञा सान्द्रकरुणा समानाधिक-वर्जिता ॥ ५१॥"
  },
  "slok52": {
    "line1": "सर्वशक्तिमयी सर्व-मङ्गला सद्गतिप्रदा ।",
    "line2": "सर्वेश्वरी सर्वमयी सर्वमन्त्र-स्वरूपिणी ॥ ५२॥"
  },
  "slok53": {
    "line1": "सर्व-यन्त्रात्मिका सर्व-तन्त्ररूपा मनोन्मनी ।",
    "line2": "माहेश्वरी महादेवी महालक्ष्मीर् मृडप्रिया ॥ ५३॥"
  },
  "slok54": {
    "line1": "महारूपा महापूज्या महापातक-नाशिनी ।",
    "line2": "महामाया महासत्त्वा महाशक्तिर् महारतिः ॥ ५४॥"
  },
  "slok55": {
    "line1": "महाभोगा महैश्वर्या महावीर्या महाबला ।",
    "line2": "महाबुद्धिर् महासिद्धिर् महायोगेश्वरेश्वरी ॥ ५५॥"
  },
  "slok56": {
    "line1": "महातन्त्रा महामन्त्रा महायन्त्रा महासना ।",
    "line2": "महायाग-क्रमाराध्या महाभैरव-पूजिता ॥ ५६॥"
  },
  "slok57": {
    "line1": "महेश्वर-महाकल्प-महाताण्डव-साक्षिणी ।",
    "line2": "महाकामेश-महिषी महात्रिपुर-सुन्दरी ॥ ५७॥"
  },
  "slok58": {
    "line1": "चतुःषष्ट्युपचाराढ्या चतुःषष्टिकलामयी ।",
    "line2": "महाचतुः-षष्टिकोटि-योगिनी-गणसेविता ॥ ५८॥"
  },
  "slok59": {
    "line1": "मनुविद्या चन्द्रविद्या चन्द्रमण्डल-मध्यगा ।",
    "line2": "चारुरूपा चारुहासा चारुचन्द्र-कलाधरा ॥ ५९॥"
  },
  "slok60": {
    "line1": "चराचर-जगन्नाथा चक्रराज-निकेतना ।",
    "line2": "पार्वती पद्मनयना पद्मराग-समप्रभा ॥ ६०॥"
  },
  "slok61": {
    "line1": "पञ्च-प्रेतासनासीना पञ्चब्रह्म-स्वरूपिणी ।",
    "line2": "चिन्मयी परमानन्दा विज्ञान-घनरूपिणी ॥ ६१॥"
  },
  "slok62": {
    "line1": "ध्यान-ध्यातृ-ध्येयरूपा धर्माधर्म-विवर्जिता ।",
    "line2": "विश्वरूपा जागरिणी स्वपन्ती तैजसात्मिका ॥ ६२॥"
  },
  "slok63": {
    "line1": "सुप्ता प्राज्ञात्मिका तुर्या सर्वावस्था-विवर्जिता ।",
    "line2": "सृष्टिकर्त्री ब्रह्मरूपा गोप्त्री गोविन्दरूपिणी ॥ ६३॥"
  },
  "slok64": {
    "line1": "संहारिणी रुद्ररूपा तिरोधान-करीश्वरी ।",
    "line2": "सदाशिवाऽनुग्रहदा पञ्चकृत्य-परायणा ॥ ६४॥"
  },
  "slok65": {
    "line1": "भानुमण्डल-मध्यस्था भैरवी भगमालिनी ।",
    "line2": "पद्मासना भगवती पद्मनाभ-सहोदरी ॥ ६५॥"
  },
  "slok66": {
    "line1": "उन्मेष-निमिषोत्पन्न-विपन्न-भुवनावली ।",
    "line2": "सहस्र-शीर्षवदना सहस्राक्षी सहस्रपात् ॥ ६६॥"
  },
  "slok67": {
    "line1": "आब्रह्म-कीट-जननी वर्णाश्रम-विधायिनी ।",
    "line2": "निजाज्ञारूप-निगमा पुण्यापुण्य-फलप्रदा ॥ ६७॥"
  },
  "slok68": {
    "line1": "श्रुति-सीमन्त-सिन्दूरी-कृत-पादाब्ज-धूलिका ।",
    "line2": "सकलागम-सन्दोह-शुक्ति-सम्पुट-मौक्तिका ॥ ६८॥"
  },
  "slok69": {
    "line1": "पुरुषार्थप्रदा पूर्णा भोगिनी भुवनेश्वरी ।",
    "line2": "अम्बिकाऽनादि-निधना हरिब्रह्मेन्द्र-सेविता ॥ ६९॥"
  },
  "slok70": {
    "line1": "नारायणी नादरूपा नामरूप-विवर्जिता ।",
    "line2": "ह्रींकारी ह्रीमती हृद्या हेयोपादेय-वर्जिता ॥ ७०॥"
  },
  "slok71": {
    "line1": "राजराजार्चिता राज्ञी रम्या राजीवलोचना ।",
    "line2": "रञ्जनी रमणी रस्या रणत्किङ्किणि-मेखला ॥ ७१॥"
  },
  "slok72": {
    "line1": "रमा राकेन्दुवदना रतिरूपा रतिप्रिया ।",
    "line2": "रक्षाकरी राक्षसघ्नी रामा रमणलम्पटा ॥ ७२॥"
  },
  "slok73": {
    "line1": "काम्या कामकलारूपा कदम्ब-कुसुम-प्रिया ।",
    "line2": "कल्याणी जगतीकन्दा करुणा-रस-सागरा ॥ ७३॥"
  },
  "slok74": {
    "line1": "कलावती कलालापा कान्ता कादम्बरीप्रिया ।",
    "line2": "वरदा वामनयना वारुणी-मद-विह्वला ॥ ७४॥"
  },
  "slok75": {
    "line1": "विश्वाधिका वेदवेद्या विन्ध्याचल-निवासिनी ।",
    "line2": "विधात्री वेदजननी विष्णुमाया विलासिनी ॥ ७५॥"
  },
  "slok76": {
    "line1": "क्षेत्रस्वरूपा क्षेत्रेशी क्षेत्र-क्षेत्रज्ञ-पालिनी ।",
    "line2": "क्षयवृद्धि-विनिर्मुक्ता क्षेत्रपाल-समर्चिता ॥ ७६॥"
  },
  "slok77": {
    "line1": "विजया विमला वन्द्या वन्दारु-जन-वत्सला ।",
    "line2": "वाग्वादिनी वामकेशी वह्निमण्डल-वासिनी ॥ ७७॥"
  },
  "slok78": {
    "line1": "भक्तिमत्-कल्पलतिका पशुपाश-विमोचिनी ।",
    "line2": "संहृताशेष-पाषण्डा सदाचार-प्रवर्तिका ॥ ७८॥ var. पाखण्डा"
  },
  "slok79": {
    "line1": "तापत्रयाग्नि-सन्तप्त-समाह्लादन-चन्द्रिका ।",
    "line2": "तरुणी तापसाराध्या तनुमध्या तमोऽपहा ॥ ७९॥"
  },
  "slok80": {
    "line1": "चितिस्तत्पद-लक्ष्यार्था चिदेकरस-रूपिणी ।",
    "line2": "स्वात्मानन्द-लवीभूत-ब्रह्माद्यानन्द-सन्ततिः ॥ ८०॥"
  },
  "slok81": {
    "line1": "परा प्रत्यक्चितीरूपा पश्यन्ती परदेवता ।",
    "line2": "मध्यमा वैखरीरूपा भक्त-मानस-हंसिका ॥ ८१॥"
  },
  "slok82": {
    "line1": "कामेश्वर-प्राणनाडी कृतज्ञा कामपूजिता ।",
    "line2": "श‍ृङ्गार-रस-सम्पूर्णा जया जालन्धर-स्थिता ॥ ८२॥"
  },
  "slok83": {
    "line1": "ओड्याणपीठ-निलया बिन्दु-मण्डलवासिनी ।",
    "line2": "रहोयाग-क्रमाराध्या रहस्तर्पण-तर्पिता ॥ ८३॥"
  },
  "slok84": {
    "line1": "सद्यःप्रसादिनी विश्व-साक्षिणी साक्षिवर्जिता ।",
    "line2": "षडङ्गदेवता-युक्ता षाड्गुण्य-परिपूरिता ॥ ८४॥"
  },
  "slok85": {
    "line1": "नित्यक्लिन्ना निरुपमा निर्वाण-सुख-दायिनी ।",
    "line2": "नित्या-षोडशिका-रूपा श्रीकण्ठार्ध-शरीरिणी ॥ ८५॥"
  },
  "slok86": {
    "line1": "प्रभावती प्रभारूपा प्रसिद्धा परमेश्वरी ।",
    "line2": "मूलप्रकृतिर् अव्यक्ता व्यक्ताव्यक्त-स्वरूपिणी ॥ ८६॥"
  },
  "slok87": {
    "line1": "व्यापिनी विविधाकारा विद्याविद्या-स्वरूपिणी ।",
    "line2": "महाकामेश-नयन-कुमुदाह्लाद-कौमुदी ॥ ८७॥"
  },
  "slok88": {
    "line1": "भक्त-हार्द-तमोभेद-भानुमद्भानु-सन्ततिः ।",
    "line2": "शिवदूती शिवाराध्या शिवमूर्तिः शिवङ्करी ॥ ८८॥"
  },
  "slok89": {
    "line1": "शिवप्रिया शिवपरा शिष्टेष्टा शिष्टपूजिता ।",
    "line2": "अप्रमेया स्वप्रकाशा मनोवाचामगोचरा ॥ ८९॥"
  },
  "slok90": {
    "line1": "चिच्छक्तिश् चेतनारूपा जडशक्तिर् जडात्मिका ।",
    "line2": "गायत्री व्याहृतिः सन्ध्या द्विजवृन्द-निषेविता ॥ ९०॥"
  },
  "slok91": {
    "line1": "तत्त्वासना तत्त्वमयी पञ्च-कोशान्तर-स्थिता ।",
    "line2": "निःसीम-महिमा नित्य-यौवना मदशालिनी ॥ ९१॥ var. निस्सीम"
  },
  "slok92": {
    "line1": "मदघूर्णित-रक्ताक्षी मदपाटल-गण्डभूः ।",
    "line2": "चन्दन-द्रव-दिग्धाङ्गी चाम्पेय-कुसुम-प्रिया ॥ ९२॥"
  },
  "slok93": {
    "line1": "कुशला कोमलाकारा कुरुकुल्ला कुलेश्वरी ।",
    "line2": "कुलकुण्डालया कौल-मार्ग-तत्पर-सेविता ॥ ९३॥"
  },
  "slok94": {
    "line1": "कुमार-गणनाथाम्बा तुष्टिः पुष्टिर् मतिर् धृतिः ।",
    "line2": "शान्तिः स्वस्तिमती कान्तिर् नन्दिनी विघ्ननाशिनी ॥ ९४॥"
  },
  "slok95": {
    "line1": "तेजोवती त्रिनयना लोलाक्षी-कामरूपिणी ।",
    "line2": "मालिनी हंसिनी माता मलयाचल-वासिनी ॥ ९५॥"
  },
  "slok96": {
    "line1": "सुमुखी नलिनी सुभ्रूः शोभना सुरनायिका ।",
    "line2": "कालकण्ठी कान्तिमती क्षोभिणी सूक्ष्मरूपिणी ॥ ९६॥"
  },
  "slok97": {
    "line1": "वज्रेश्वरी वामदेवी वयोऽवस्था-विवर्जिता ।",
    "line2": "सिद्धेश्वरी सिद्धविद्या सिद्धमाता यशस्विनी ॥ ९७॥"
  },
  "slok98": {
    "line1": "विशुद्धिचक्र-निलयाऽऽरक्तवर्णा त्रिलोचना ।",
    "line2": "खट्वाङ्गादि-प्रहरणा वदनैक-समन्विता ॥ ९८॥"
  },
  "slok99": {
    "line1": "पायसान्नप्रिया त्वक्स्था पशुलोक-भयङ्करी ।",
    "line2": "अमृतादि-महाशक्ति-संवृता डाकिनीश्वरी ॥ ९९॥"
  },
  "slok100": {
    "line1": "अनाहताब्ज-निलया श्यामाभा वदनद्वया ।",
    "line2": "दंष्ट्रोज्ज्वलाऽक्ष-मालादि-धरा रुधिरसंस्थिता ॥ १००॥"
  },
  "slok101": {
    "line1": "कालरात्र्यादि-शक्त्यौघ-वृता स्निग्धौदनप्रिया ।",
    "line2": "महावीरेन्द्र-वरदा राकिण्यम्बा-स्वरूपिणी ॥ १०१॥"
  },
  "slok102": {
    "line1": "मणिपूराब्ज-निलया वदनत्रय-संयुता ।",
    "line2": "वज्रादिकायुधोपेता डामर्यादिभिरावृता ॥ १०२॥"
  },
  "slok103": {
    "line1": "रक्तवर्णा मांसनिष्ठा गुडान्न-प्रीत-मानसा ।",
    "line2": "समस्तभक्त-सुखदा लाकिन्यम्बा-स्वरूपिणी ॥ १०३॥"
  },
  "slok104": {
    "line1": "स्वाधिष्ठानाम्बुज-गता चतुर्वक्त्र-मनोहरा ।",
    "line2": "शूलाद्यायुध-सम्पन्ना पीतवर्णाऽतिगर्विता ॥ १०४॥"
  },
  "slok105": {
    "line1": "मेदोनिष्ठा मधुप्रीता बन्धिन्यादि-समन्विता ।",
    "line2": "दध्यन्नासक्त-हृदया काकिनी-रूप-धारिणी ॥ १०५॥"
  },
  "slok106": {
    "line1": "मूलाधाराम्बुजारूढा पञ्च-वक्त्राऽस्थि-संस्थिता ।",
    "line2": "अङ्कुशादि-प्रहरणा वरदादि-निषेविता ॥ १०६॥"
  },
  "slok107": {
    "line1": "मुद्गौदनासक्त-चित्ता साकिन्यम्बा-स्वरूपिणी ।",
    "line2": "आज्ञा-चक्राब्ज-निलया शुक्लवर्णा षडानना ॥ १०७॥"
  },
  "slok108": {
    "line1": "मज्जासंस्था हंसवती-मुख्य-शक्ति-समन्विता ।",
    "line2": "हरिद्रान्नैक-रसिका हाकिनी-रूप-धारिणी ॥ १०८॥"
  },
  "slok109": {
    "line1": "सहस्रदल-पद्मस्था सर्व-वर्णोप-शोभिता ।",
    "line2": "सर्वायुधधरा शुक्ल-संस्थिता सर्वतोमुखी ॥ १०९॥"
  },
  "slok110": {
    "line1": "सर्वौदन-प्रीतचित्ता याकिन्यम्बा-स्वरूपिणी ।",
    "line2": "स्वाहा स्वधाऽमतिर् मेधा श्रुतिः स्मृतिर् अनुत्तमा ॥ ११०॥"
  },
  "slok111": {
    "line1": "पुण्यकीर्तिः पुण्यलभ्या पुण्यश्रवण-कीर्तना ।",
    "line2": "पुलोमजार्चिता बन्ध-मोचनी बन्धुरालका ॥ १११॥ var. मोचनी बर्बरालका"
  },
  "slok112": {
    "line1": "विमर्शरूपिणी विद्या वियदादि-जगत्प्रसूः ।",
    "line2": "सर्वव्याधि-प्रशमनी सर्वमृत्यु-निवारिणी ॥ ११२॥"
  },
  "slok113": {
    "line1": "अग्रगण्याऽचिन्त्यरूपा कलिकल्मष-नाशिनी ।",
    "line2": "कात्यायनी कालहन्त्री कमलाक्ष-निषेविता ॥ ११३॥"
  },
  "slok114": {
    "line1": "ताम्बूल-पूरित-मुखी दाडिमी-कुसुम-प्रभा ।",
    "line2": "मृगाक्षी मोहिनी मुख्या मृडानी मित्ररूपिणी ॥ ११४॥"
  },
  "slok115": {
    "line1": "नित्यतृप्ता भक्तनिधिर् नियन्त्री निखिलेश्वरी ।",
    "line2": "मैत्र्यादि-वासनालभ्या महाप्रलय-साक्षिणी ॥ ११५॥"
  },
  "slok116": {
    "line1": "परा शक्तिः परा निष्ठा प्रज्ञानघन-रूपिणी ।",
    "line2": "माध्वीपानालसा मत्ता मातृका-वर्ण-रूपिणी ॥ ११६॥"
  },
  "slok117": {
    "line1": "महाकैलास-निलया मृणाल-मृदु-दोर्लता ।",
    "line2": "महनीया दयामूर्तिर् महासाम्राज्य-शालिनी ॥ ११७॥"
  },
  "slok118": {
    "line1": "आत्मविद्या महाविद्या श्रीविद्या कामसेविता ।",
    "line2": "श्री-षोडशाक्षरी-विद्या त्रिकूटा कामकोटिका ॥ ११८॥"
  },
  "slok119": {
    "line1": "कटाक्ष-किङ्करी-भूत-कमला-कोटि-सेविता ।",
    "line2": "शिरःस्थिता चन्द्रनिभा भालस्थेन्द्र-धनुःप्रभा ॥ ११९॥"
  },
  "slok120": {
    "line1": "हृदयस्था रविप्रख्या त्रिकोणान्तर-दीपिका ।",
    "line2": "दाक्षायणी दैत्यहन्त्री दक्षयज्ञ-विनाशिनी ॥ १२०॥"
  },
  "slok121": {
    "line1": "दरान्दोलित-दीर्घाक्षी दर-हासोज्ज्वलन्-मुखी ।",
    "line2": "गुरुमूर्तिर् गुणनिधिर् गोमाता गुहजन्मभूः ॥ १२१॥"
  },
  "slok122": {
    "line1": "देवेशी दण्डनीतिस्था दहराकाश-रूपिणी ।",
    "line2": "प्रतिपन्मुख्य-राकान्त-तिथि-मण्डल-पूजिता ॥ १२२॥"
  },
  "slok123": {
    "line1": "कलात्मिका कलानाथा काव्यालाप-विनोदिनी । var. विमोदिनी",
    "line2": "सचामर-रमा-वाणी-सव्य-दक्षिण-सेविता ॥ १२३॥"
  },
  "slok124": {
    "line1": "आदिशक्तिर् अमेयाऽऽत्मा परमा पावनाकृतिः ।",
    "line2": "अनेककोटि-ब्रह्माण्ड-जननी दिव्यविग्रहा ॥ १२४॥"
  },
  "slok125": {
    "line1": "क्लींकारी केवला गुह्या कैवल्य-पददायिनी ।",
    "line2": "त्रिपुरा त्रिजगद्वन्द्या त्रिमूर्तिस् त्रिदशेश्वरी ॥ १२५॥"
  },
  "slok126": {
    "line1": "त्र्यक्षरी दिव्य-गन्धाढ्या सिन्दूर-तिलकाञ्चिता ।",
    "line2": "उमा शैलेन्द्रतनया गौरी गन्धर्व-सेविता ॥ १२६॥"
  },
  "slok127": {
    "line1": "विश्वगर्भा स्वर्णगर्भाऽवरदा वागधीश्वरी ।",
    "line2": "ध्यानगम्याऽपरिच्छेद्या ज्ञानदा ज्ञानविग्रहा ॥ १२७॥"
  },
  "slok128": {
    "line1": "सर्ववेदान्त-संवेद्या सत्यानन्द-स्वरूपिणी ।",
    "line2": "लोपामुद्रार्चिता लीला-कॢप्त-ब्रह्माण्ड-मण्डला ॥ १२८॥"
  },
  "slok129": {
    "line1": "अदृश्या दृश्यरहिता विज्ञात्री वेद्यवर्जिता ।",
    "line2": "योगिनी योगदा योग्या योगानन्दा युगन्धरा ॥ १२९॥"
  },
  "slok130": {
    "line1": "इच्छाशक्ति-ज्ञानशक्ति-क्रियाशक्ति-स्वरूपिणी ।",
    "line2": "सर्वाधारा सुप्रतिष्ठा सदसद्रूप-धारिणी ॥ १३०॥"
  },
  "slok131": {
    "line1": "अष्टमूर्तिर् अजाजैत्री लोकयात्रा-विधायिनी । var. अजाजेत्री",
    "line2": "एकाकिनी भूमरूपा निर्द्वैता द्वैतवर्जिता ॥ १३१॥"
  },
  "slok132": {
    "line1": "अन्नदा वसुदा वृद्धा ब्रह्मात्मैक्य-स्वरूपिणी ।",
    "line2": "बृहती ब्राह्मणी ब्राह्मी ब्रह्मानन्दा बलिप्रिया ॥ १३२॥"
  },
  "slok133": {
    "line1": "भाषारूपा बृहत्सेना भावाभाव-विवर्जिता ।",
    "line2": "सुखाराध्या शुभकरी शोभना सुलभा गतिः ॥ १३३॥"
  },
  "slok134": {
    "line1": "राज-राजेश्वरी राज्य-दायिनी राज्य-वल्लभा ।",
    "line2": "राजत्कृपा राजपीठ-निवेशित-निजाश्रिता ॥ १३४॥"
  },
  "slok135": {
    "line1": "राज्यलक्ष्मीः कोशनाथा चतुरङ्ग-बलेश्वरी ।",
    "line2": "साम्राज्य-दायिनी सत्यसन्धा सागरमेखला ॥ १३५॥"
  },
  "slok136": {
    "line1": "दीक्षिता दैत्यशमनी सर्वलोक-वशङ्करी ।",
    "line2": "सर्वार्थदात्री सावित्री सच्चिदानन्द-रूपिणी ॥ १३६॥"
  },
  "slok137": {
    "line1": "देश-कालापरिच्छिन्ना सर्वगा सर्वमोहिनी ।",
    "line2": "सरस्वती शास्त्रमयी गुहाम्बा गुह्यरूपिणी ॥ १३७॥"
  },
  "slok138": {
    "line1": "सर्वोपाधि-विनिर्मुक्ता सदाशिव-पतिव्रता ।",
    "line2": "सम्प्रदायेश्वरी साध्वी गुरुमण्डल-रूपिणी ॥ १३८॥"
  },
  "slok139": {
    "line1": "कुलोत्तीर्णा भगाराध्या माया मधुमती मही ।",
    "line2": "गणाम्बा गुह्यकाराध्या कोमलाङ्गी गुरुप्रिया ॥ १३९॥"
  },
  "slok140": {
    "line1": "स्वतन्त्रा सर्वतन्त्रेशी दक्षिणामूर्ति-रूपिणी ।",
    "line2": "सनकादि-समाराध्या शिवज्ञान-प्रदायिनी ॥ १४०॥"
  },
  "slok141": {
    "line1": "चित्कलाऽऽनन्द-कलिका प्रेमरूपा प्रियङ्करी ।",
    "line2": "नामपारायण-प्रीता नन्दिविद्या नटेश्वरी ॥ १४१॥"
  },
  "slok142": {
    "line1": "मिथ्या-जगदधिष्ठाना मुक्तिदा मुक्तिरूपिणी ।",
    "line2": "लास्यप्रिया लयकरी लज्जा रम्भादिवन्दिता ॥ १४२॥"
  },
  "slok143": {
    "line1": "भवदाव-सुधावृष्टिः पापारण्य-दवानला ।",
    "line2": "दौर्भाग्य-तूलवातूला जराध्वान्त-रविप्रभा ॥ १४३॥"
  },
  "slok144": {
    "line1": "भाग्याब्धि-चन्द्रिका भक्त-चित्तकेकि-घनाघना ।",
    "line2": "रोगपर्वत-दम्भोलिर् मृत्युदारु-कुठारिका ॥ १४४॥"
  },
  "slok145": {
    "line1": "महेश्वरी महाकाली महाग्रासा महाशना ।",
    "line2": "अपर्णा चण्डिका चण्डमुण्डासुर-निषूदिनी ॥ १४५॥"
  },
  "slok146": {
    "line1": "क्षराक्षरात्मिका सर्व-लोकेशी विश्वधारिणी ।",
    "line2": "त्रिवर्गदात्री सुभगा त्र्यम्बका त्रिगुणात्मिका ॥ १४६॥"
  },
  "slok147": {
    "line1": "स्वर्गापवर्गदा शुद्धा जपापुष्प-निभाकृतिः ।",
    "line2": "ओजोवती द्युतिधरा यज्ञरूपा प्रियव्रता ॥ १४७॥"
  },
  "slok148": {
    "line1": "दुराराध्या दुराधर्षा पाटली-कुसुम-प्रिया ।",
    "line2": "महती मेरुनिलया मन्दार-कुसुम-प्रिया ॥ १४८॥"
  },
  "slok149": {
    "line1": "वीराराध्या विराड्रूपा विरजा विश्वतोमुखी ।",
    "line2": "प्रत्यग्रूपा पराकाशा प्राणदा प्राणरूपिणी ॥ १४९॥"
  },
  "slok150": {
    "line1": "मार्ताण्ड-भैरवाराध्या मन्त्रिणीन्यस्त-राज्यधूः । var. मार्तण्ड",
    "line2": "त्रिपुरेशी जयत्सेना निस्त्रैगुण्या परापरा ॥ १५०॥"
  },
  "slok151": {
    "line1": "सत्य-ज्ञानानन्द-रूपा सामरस्य-परायणा ।",
    "line2": "कपर्दिनी कलामाला कामधुक् कामरूपिणी ॥ १५१॥"
  },
  "slok152": {
    "line1": "कलानिधिः काव्यकला रसज्ञा रसशेवधिः ।",
    "line2": "पुष्टा पुरातना पूज्या पुष्करा पुष्करेक्षणा ॥ १५२॥"
  },
  "slok153": {
    "line1": "परंज्योतिः परंधाम परमाणुः परात्परा ।",
    "line2": "पाशहस्ता पाशहन्त्री परमन्त्र-विभेदिनी ॥ १५३॥"
  },
  "slok154": {
    "line1": "मूर्ताऽमूर्ताऽनित्यतृप्ता मुनिमानस-हंसिका ।",
    "line2": "सत्यव्रता सत्यरूपा सर्वान्तर्यामिनी सती ॥ १५४॥"
  },
  "slok155": {
    "line1": "ब्रह्माणी ब्रह्मजननी बहुरूपा बुधार्चिता ।",
    "line2": "प्रसवित्री प्रचण्डाऽऽज्ञा प्रतिष्ठा प्रकटाकृतिः ॥ १५५॥"
  },
  "slok156": {
    "line1": "प्राणेश्वरी प्राणदात्री पञ्चाशत्पीठ-रूपिणी ।",
    "line2": "विश‍ृङ्खला विविक्तस्था वीरमाता वियत्प्रसूः ॥ १५६॥"
  },
  "slok157": {
    "line1": "मुकुन्दा मुक्तिनिलया मूलविग्रह-रूपिणी ।",
    "line2": "भावज्ञा भवरोगघ्नी भवचक्र-प्रवर्तिनी ॥ १५७॥"
  },
  "slok158": {
    "line1": "छन्दःसारा शास्त्रसारा मन्त्रसारा तलोदरी ।",
    "line2": "उदारकीर्तिर् उद्दामवैभवा वर्णरूपिणी ॥ १५८॥"
  },
  "slok159": {
    "line1": "जन्ममृत्यु-जरातप्त-जनविश्रान्ति-दायिनी ।",
    "line2": "सर्वोपनिष-दुद्-घुष्टा शान्त्यतीत-कलात्मिका ॥ १५९॥"
  },
  "slok160": {
    "line1": "गम्भीरा गगनान्तस्था गर्विता गानलोलुपा ।",
    "line2": "कल्पना-रहिता काष्ठाऽकान्ता कान्तार्ध-विग्रहा ॥ १६०॥"
  },
  "slok161": {
    "line1": "कार्यकारण-निर्मुक्ता कामकेलि-तरङ्गिता ।",
    "line2": "कनत्कनकता-टङ्का लीला-विग्रह-धारिणी ॥ १६१॥"
  },
  "slok162": {
    "line1": "अजा क्षयविनिर्मुक्ता मुग्धा क्षिप्र-प्रसादिनी ।",
    "line2": "अन्तर्मुख-समाराध्या बहिर्मुख-सुदुर्लभा ॥ १६२॥"
  },
  "slok163": {
    "line1": "त्रयी त्रिवर्गनिलया त्रिस्था त्रिपुरमालिनी ।",
    "line2": "निरामया निरालम्बा स्वात्मारामा सुधासृतिः ॥ १६३॥ var. सुधास्रुतिः"
  },
  "slok164": {
    "line1": "संसारपङ्क-निर्मग्न-समुद्धरण-पण्डिता ।",
    "line2": "यज्ञप्रिया यज्ञकर्त्री यजमान-स्वरूपिणी ॥ १६४॥"
  },
  "slok165": {
    "line1": "धर्माधारा धनाध्यक्षा धनधान्य-विवर्धिनी ।",
    "line2": "विप्रप्रिया विप्ररूपा विश्वभ्रमण-कारिणी ॥ १६५॥"
  },
  "slok166": {
    "line1": "विश्वग्रासा विद्रुमाभा वैष्णवी विष्णुरूपिणी ।",
    "line2": "अयोनिर् योनिनिलया कूटस्था कुलरूपिणी ॥ १६६॥"
  },
  "slok167": {
    "line1": "वीरगोष्ठीप्रिया वीरा नैष्कर्म्या नादरूपिणी ।",
    "line2": "विज्ञानकलना कल्या विदग्धा बैन्दवासना ॥ १६७॥"
  },
  "slok168": {
    "line1": "तत्त्वाधिका तत्त्वमयी तत्त्वमर्थ-स्वरूपिणी ।",
    "line2": "सामगानप्रिया सौम्या सदाशिव-कुटुम्बिनी ॥ १६८॥ var. सोम्या"
  },
  "slok169": {
    "line1": "सव्यापसव्य-मार्गस्था सर्वापद्विनिवारिणी ।",
    "line2": "स्वस्था स्वभावमधुरा धीरा धीरसमर्चिता ॥ १६९॥"
  },
  "slok170": {
    "line1": "चैतन्यार्घ्य-समाराध्या चैतन्य-कुसुमप्रिया ।",
    "line2": "सदोदिता सदातुष्टा तरुणादित्य-पाटला ॥ १७०॥"
  },
  "slok171": {
    "line1": "दक्षिणा-दक्षिणाराध्या दरस्मेर-मुखाम्बुजा ।",
    "line2": "कौलिनी-केवलाऽनर्घ्य-कैवल्य-पददायिनी ॥ १७१॥"
  },
  "slok172": {
    "line1": "स्तोत्रप्रिया स्तुतिमती श्रुति-संस्तुत-वैभवा ।",
    "line2": "मनस्विनी मानवती महेशी मङ्गलाकृतिः ॥ १७२॥"
  },
  "slok173": {
    "line1": "विश्वमाता जगद्धात्री विशालाक्षी विरागिणी ।",
    "line2": "प्रगल्भा परमोदारा परामोदा मनोमयी ॥ १७३॥"
  },
  "slok174": {
    "line1": "व्योमकेशी विमानस्था वज्रिणी वामकेश्वरी ।",
    "line2": "पञ्चयज्ञ-प्रिया पञ्च-प्रेत-मञ्चाधिशायिनी ॥ १७४॥"
  },
  "slok175": {
    "line1": "पञ्चमी पञ्चभूतेशी पञ्च-संख्योपचारिणी ।",
    "line2": "शाश्वती शाश्वतैश्वर्या शर्मदा शम्भुमोहिनी ॥ १७५॥"
  },
  "slok176": {
    "line1": "धराधरसुता धन्या धर्मिणी धर्मवर्धिनी ।",
    "line2": "लोकातीता गुणातीता सर्वातीता शमात्मिका ॥ १७६॥"
  },
  "slok177": {
    "line1": "बन्धूक-कुसुमप्रख्या बाला लीलाविनोदिनी ।",
    "line2": "सुमङ्गली सुखकरी सुवेषाढ्या सुवासिनी ॥ १७७॥"
  },
  "slok178": {
    "line1": "सुवासिन्यर्चन-प्रीताऽऽशोभना शुद्धमानसा ।",
    "line2": "बिन्दु-तर्पण-सन्तुष्टा पूर्वजा त्रिपुराम्बिका ॥ १७८॥"
  },
  "slok179": {
    "line1": "दशमुद्रा-समाराध्या त्रिपुराश्री-वशङ्करी ।",
    "line2": "ज्ञानमुद्रा ज्ञानगम्या ज्ञानज्ञेय-स्वरूपिणी ॥ १७९॥"
  },
  "slok180": {
    "line1": "योनिमुद्रा त्रिखण्डेशी त्रिगुणाम्बा त्रिकोणगा ।",
    "line2": "अनघाऽद्भुत-चारित्रा वाञ्छितार्थ-प्रदायिनी ॥ १८०॥"
  },
  "slok181": {
    "line1": "अभ्यासातिशय-ज्ञाता षडध्वातीत-रूपिणी ।",
    "line2": "अव्याज-करुणा-मूर्तिर् अज्ञान-ध्वान्त-दीपिका ॥ १८१॥"
  },
  "slok182": {
    "line1": "आबाल-गोप-विदिता सर्वानुल्लङ्घ्य-शासना ।",
    "line2": "श्रीचक्रराज-निलया श्रीमत्-त्रिपुरसुन्दरी ॥ १८२॥"
  },
  "slok183": {
    "line1": "श्रीशिवा शिव-शक्त्यैक्य-रूपिणी ललिताम्बिका ।",
    "line2": "एवं श्रीललिता देव्या नाम्नां साहस्रकं जगुः ॥"
  },
  "slok184": {
    "line1": "॥ इति श्रीब्रह्माण्डपुराणे उत्तरखण्डे श्रीहयग्रीवागस्त्यसंवादे",
    "line2": "श्रीललिता सहस्रनाम स्तोत्र कथनं सम्पूर्णम् ॥"
  }
}